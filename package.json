{
	"name": "ivorytower",
	"version": "3.0.0-beta",
	"private": true,
	"license": "UNLICENSED",
	"dependencies": {
		"@travisspomer/tidbits": "^1.0.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "^1.0.0-next.322",
		"@types/html-minifier": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.13.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"html-minifier": "^4.0.0",
		"sass": "^1.51.0",
		"svelte": "^3.47.0",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.4.0",
		"typescript": "^4.6.3"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"scripts": {
		"start": "svelte-kit dev --port 80 --open --host",
		"startwithapi": "swa start --api ./api --port 80 http://localhost:3000 --run=\"npm exec svelte-kit dev\"",
		"startwithvs": "swa start --api http://localhost:7071 --port 80 http://localhost:3000 --run=\"npm exec svelte-kit dev\"",
		"build": "svelte-kit sync && tsc --noEmit && svelte-kit build",
		"serve": "svelte-kit preview --port 80 --open --host"
	}
